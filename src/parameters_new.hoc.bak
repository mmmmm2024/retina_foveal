//======================================
// Simulation Parameters
//======================================
ENABLE_GRAPHICAL_INTERFACE = 1
tstop           = 6000        // (ms) total simulation time 6000ms
step_dt         = 0.0625     // (ms) simulation step (original: 0.005)

//======================================
// Cell Parameters
//--------------------------------------
//  Rod, Cone
//   Rod Bipolar (RBC),ON Cone Bipolar (ONCBC), OFF Cone Bipolar (OFFCBC)
//   AIIAC(AC)
//  ON Ganglion (ONGC), OFF Ganglion (OFFGC)
//======================================

//--------------------------------------
// Rod
//--------------------------------------
Num_R           = 1
// (Publio: 1500)
noise_mean_R    = 0.003
//0.003(default)
noise_std_R     = 0.0004
tau_noise_R     = 5.0
seed_noise_R    = 1
kr              = 0           // coupling constant (0~4)
R_cov           = 10         // coverage R2RB
//-------------
//input to Rod
//-------------
//Input_rod
AMP             = 50          
stim            = 2000    //del 刺激開始時間
num_stim        = 1       //イベント回数
ton_stim        = 1000    //刺激時間
toff_stim       = 0       //無刺激時間

//InputDim_rod
stim_Dim        = stim
num_Dim         = num_stim
ton_Dim         = ton_stim
toff_Dim        = toff_stim
ssI_Dim         = 0       
amp_Dim         = 0

// del        = 2000        // (ms) 刺激開始時刻
// on         = 6000        // (ms) 刺激終了時刻
// gihbar     = 2.5

//--------------------------------------
// Cone
//--------------------------------------
Num_C           = 20
Num_C_RP        = 0
//(Publio: 16)
noise_mean_C    = 0.003
noise_std_C     = 0.0004
tau_noise_C     = 5.0
seed_noise_C    = 3
C_cov           = 4           // coverage
//---------------
//Input to Cones
//---------------
AMP_C           = AMP
stim_C          = stim
num_stim2C      = num_stim
ton_C           = ton_stim
toff_C          = toff_stim
ssI_C           = 40

//--------------------------------------
// AII Amacrine Cell (AIIAC)
//--------------------------------------
Num_AC          = 8
//4
//1
noise_mean_AC   = 0.003
noise_std_AC    = 0.0004
tau_noise_AC    = 5.0
seed_noise_AC   = 5
ka              = 0           // coupling constant (0~4)

//--------------------------------------
// Rod Bipolar (RBC)
//--------------------------------------
Num_RBC         = 32
//16
//4
// 例: Publioでは100, Tadaでは126, paperでは173など
noise_mean_RBC  = 0.003
noise_std_RBC   = 0.0004
tau_noise_RBC   = 5.0
seed_noise_RBC  = 3

//--------------------------------------
// ON Cone Bipolar (ONCBC)
//--------------------------------------
Num_ONCBC           = 20  
//(Fovea)11
// 論文により28, 55など
noise_mean_ONCBC    = 0.003
noise_std_ONCBC     = 0.0004
tau_noise_ONCBC     = 5.0
seed_noise_ONCBC    = 5

//--------------------------------------
// OFF Cone Bipolar (OFFCBC)
//--------------------------------------
Num_OFFCBC          = 20
//24
noise_mean_OFFCBC   = 0.003
noise_std_OFFCBC    = 0.0004
tau_noise_OFFCBC    = 5.0
seed_noise_OFFCBC   = 5

//--------------------------------------
// ON Ganglion (ONGC)
//--------------------------------------
Num_ONGC            = 20
noise_mean_ONGC     = 0.003   // (ex: 0.005)
noise_std_ONGC      = 0.0004  // (ex: 0.002)
tau_noise_ONGC      = 5.0
seed_noise_ONGC     = 5

//--------------------------------------
// OFF Ganglion (OFFGC)
//--------------------------------------
Num_OFFGC           = 20      // (origin=5)
noise_mean_OFFGC    = 0.003
noise_std_OFFGC     = 0.005
tau_noise_OFFGC     = 5.0
seed_noise_OFFGC    = 5 //(origin=5)



//======================================
// Connection Parameters
//--------------------------------------
//   Gap Junction
//     - AIIAC <-> ONCB
//     - AIIAC <-> AIIAC
//     - OFFGC <-> OFFGC
//     - R <-> R, R <-> C など
//   Glu syn (Ex)
//     - Rod -> AIIAC
//     - ONCB -> ONGC
//     - OFFCB -> OFFGC
//     - OFFCB -> AIIAC
//   Gly syn (Inh)
//     - AIIAC -> OFFGC
//     - AIIAC -> OFFCB
//======================================

//--------------------------------------
// Gap Junction
//--------------------------------------
// AIIAC <-> ONCB
gj_AC_ONCB          = 0.00010 * 4.2   // original 0.00050
// Ahnelt et al., gj=100 pS


// R <-> R
gj_C2C              = 0.0001 * 4.8 * 3.2 / 4.1    
//[uS] Tsukamoto et al
// R <-> C
gj_R2C              = 0.0001 * 5     // [uS]
// AIIAC <-> AIIAC
gj_AC2AC            = 0.00008    // [uS]
//gj_AC2AC            = 0.0

// AIIAC <-> Cone Bipolar
gj_AC2CB            = 0.0005

// OFFGC <-> OFFGC
gj_OFFGC2OFFGC      = 0.001      // [uS]
// ONCB <-> ONCB
gj_ONCB2ONCB        = 0.001     // [uS]
// OFFCB <-> OFFCB
gj_OFFCB2OFFCB      = 0.001      // [uS]

